<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:giddyhero.soccersystem.client.mobile.widget"
	xmlns:f="urn:import:com.googlecode.mgwt.ui.client.widget.panel.flex"
	xmlns:h="urn:import:com.googlecode.mgwt.ui.client.widget.header"
	xmlns:s="urn:import:com.googlecode.mgwt.ui.client.widget.panel.scroll"
	xmlns:i="urn:import:com.googlecode.mgwt.ui.client.widget.button.image">

	<ui:with field='res'
		type='giddyhero.soccersystem.client.mobile.resources.ClientBundleMobile' />
	<ui:style>
	</ui:style>
	<f:RootFlexPanel ui:field="root">
		<g:HorizontalPanel height="100%" width="100%">
			<g:VerticalPanel ui:field="menu" visible="true"
				height="100%" width="100%">
				<g:HorizontalPanel height="30%" width="100%">
					<g:Image ui:field="imgLogo"></g:Image>
					<g:Label ui:field="lbLogo">Soccer Network</g:Label>
				</g:HorizontalPanel>
				<g:FlowPanel height="10%">
					<m:MImageButton icon='{res.btMyPage}' styleName='{res.style.button}'
						text="My Page"></m:MImageButton>
				</g:FlowPanel>

				<m:MImageButton icon='{res.btMyPage}' styleName='{res.style.button}'
					text="My Page"></m:MImageButton>
				<m:MImageButton icon='{res.btMyPage}' styleName='{res.style.button}'
					text="My Page"></m:MImageButton>
				<m:MImageButton icon='{res.btMyPage}' styleName='{res.style.button}'
					text="My Page"></m:MImageButton>
				<m:MImageButton icon='{res.btMyPage}' styleName='{res.style.button}'
					text="My Page"></m:MImageButton>
				<m:MImageButton icon='{res.btMyPage}' styleName='{res.style.button}'
					text="My Page"></m:MImageButton>
				<m:MImageButton icon='{res.btMyPage}' styleName='{res.style.button}'
					text="My Page"></m:MImageButton>
				<m:MImageButton icon='{res.btMyPage}' styleName='{res.style.button}'
					text="My Page"></m:MImageButton>
			</g:VerticalPanel>
			<g:FlowPanel ui:field="main" height="100%">
				<h:HeaderPanel ui:field="headerPanel" height="10%">
					<i:PreviousitemImageButton ui:field="headerBackButton"></i:PreviousitemImageButton>
					<f:FlexSpacer></f:FlexSpacer>
					<h:HeaderTitle ui:field="headerTitle"></h:HeaderTitle>
					<f:FlexSpacer></f:FlexSpacer>
				</h:HeaderPanel>
				<s:ScrollPanel ui:field="scrollPanel" showScrollBarX="false"
					showScrollBarY="true" width="100%" height="90%"></s:ScrollPanel>
			</g:FlowPanel>
		</g:HorizontalPanel>
	</f:RootFlexPanel>
</ui:UiBinder> 