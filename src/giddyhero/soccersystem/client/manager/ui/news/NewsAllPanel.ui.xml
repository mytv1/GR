<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
		font-weight: bold;
		}
		.table-style {
		border: 1px
		solid black;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel>
			<g:FlexTable ui:field="tblNews" styleName="{style.table-style}"
				borderWidth="2"></g:FlexTable>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 